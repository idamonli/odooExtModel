        </record>

        <record id="crm_stated_work_1_1" model="crm.stated.work">
            <field name="c_stated">find</field>
            <field name="content">掌握客户的基本资料（行业、背景、规模以及人员等）</field>
        </record>
        <record id="crm_stated_work_1_2" model="crm.stated.work">
            <field name="c_stated">find</field>
            <field name="content">评估客户的需求与公司产品的匹配度</field>
        </record>
        <record id="crm_stated_work_1_3" model="crm.stated.work">
            <field name="c_stated">find</field>
            <field name="content">介绍公司产品以及解决方案（引导客户）</field>
        </record>
        <record id="crm_stated_work_1_4" model="crm.stated.work">
            <field name="c_stated">find</field>
            <field name="content">了解客户的需求和异议</field>
        </record>
        <record id="crm_stated_work_2_1" model="crm.stated.work">
            <field name="c_stated">confirm</field>
            <field name="content">确认项目立项以及项目预算</field>
        </record>
        <record id="crm_stated_work_2_2" model="crm.stated.work">
            <field name="c_stated">confirm</field>
            <field name="content">了解项目时间进度安排</field>
        </record>
        <record id="crm_stated_work_2_3" model="crm.stated.work">
            <field name="c_stated">confirm</field>
            <field name="content">掌握项目组成员名单、及管件决策人</field>
        </record>
        <record id="crm_stated_work_2_4" model="crm.stated.work">
            <field name="c_stated">confirm</field>
            <field name="content">掌握竞争对手名单</field>
        </record>
        <record id="crm_stated_work_3_1" model="crm.stated.work">
            <field name="c_stated">solve</field>
            <field name="content">根据客户需求有针对性的提供产品服务和解决方案</field>
        </record>
        <record id="crm_stated_work_3_2" model="crm.stated.work">
            <field name="c_stated">solve</field>
            <field name="content">与项目组成员紧密沟通，掌握项目决策人对公司产品的看法</field>
        </record>
        <record id="crm_stated_work_3_3" model="crm.stated.work">
            <field name="c_stated">solve</field>
            <field name="content">异议的排除和应对策略</field>
        </record>
        <record id="crm_stated_work_3_4" model="crm.stated.work">
            <field name="c_stated">solve</field>
            <field name="content">争取主导标书需求与指标</field>
        </record>
        <record id="crm_stated_work_4_1" model="crm.stated.work">
            <field name="c_stated">talk</field>
            <field name="content">确认客户对价格的敏感度</field>
        </record>
        <record id="crm_stated_work_4_2" model="crm.stated.work">
            <field name="c_stated">talk</field>
            <field name="content">确认最主要的竞争对手以及竞争分析</field>
        </record>
        <record id="crm_stated_work_4_3" model="crm.stated.work">
            <field name="c_stated">talk</field>
            <field name="content">提交商务正式报价</field>
        </record>
        <record id="crm_stated_work_4_4" model="crm.stated.work">
            <field name="c_stated">talk</field>
            <field name="content">确认招投标时间以及计划</field>
        </record>
        <record id="crm_stated_work_5_1" model="crm.stated.work">
            <field name="c_stated">bid</field>
            <field name="content">时刻关注招标相关动态</field>
        </record>
        <record id="crm_stated_work_5_2" model="crm.stated.work">
            <field name="c_stated">bid</field>
            <field name="content">分析评分标准</field>
        </record>
        <record id="crm_stated_work_5_3" model="crm.stated.work">
            <field name="c_stated">bid</field>
            <field name="content">有针对性的按照评分标准制作投标书</field>
        </record>
        <record id="crm_stated_work_6_1" model="crm.stated.work">
            <field name="c_stated">clinch</field>
            <field name="content">签订合同</field>
        </record>
        <record id="crm_stated_work_6_2" model="crm.stated.work">
            <field name="c_stated">clinch</field>
            <field name="content">确认客户已按照合同规定支付相应款项</field>
        </record>
        <record id="crm_stated_work_6_3" model="crm.stated.work">
            <field name="c_stated">clinch</field>
            <field name="content">推动订单进展</field>
        </record>
        <record id="crm_stated_work_6_4" model="crm.stated.work">
            <field name="c_stated">clinch</field>
            <field name="content">项目总结分析</field>
        </record>

